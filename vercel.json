{
    "version": 2,
    "builds": [
        {
            "src": "index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api-anggota",
            "dest": "index.js"
        },
        {
            "src": "/members",
            "dest": "index.js"
        },
        {
            "src": "/add",
            "dest": "index.js"
        },
        {
            "src": "/delete/(.*)",
            "dest": "index.js"
        },
        {
            "src": "/(.*)",
            "dest": "/public/$1"
        }
    ],
    "files": [
        "database/anggota.json"
    ]
}
